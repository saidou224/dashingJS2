{
  "name": "dashing-js2",
  "version": "0.0.0",
  "license": "MIT",
  "scripts": {
    "ng": "ng",
    "start": "ng serve",
    "build": "ng build --prod",
    "test": "ng test",
    "lint": "ng lint",
    "e2e": "ng e2e",
    "cc": "npm run prettier && npm run lint",
    "prettier": "prettier --write src/**/*{ts,scss,md}",
    "precommit": "npm run pretty-quick --staged && npm run lint && npm run commitlint",
    "commitlint": "commitlint -e",
    "test:cover": "rm -rf ./coverage && ng test --no-watch --code-coverage",
    "coveralls": "cat ./coverage/lcov.info | ./node_modules/coveralls/bin/coveralls.js",
    "build:gh-pages:demo1": "ng build -e=demo1 -op=gh-pages/demo1/ -bh=/dashingJS2/demo1/",
    "pretty-quick": "pretty-quick"
  },
  "private": true,
  "dependencies": {
    "@angular/animations": "^5.2.8",
    "@angular/common": "^5.2.8",
    "@angular/compiler": "^5.2.8",
    "@angular/core": "^5.2.8",
    "@angular/forms": "^5.2.8",
    "@angular/http": "^5.2.8",
    "@angular/platform-browser": "^5.2.8",
    "@angular/platform-browser-dynamic": "^5.2.8",
    "@angular/router": "^5.2.8",
    "@ng-bootstrap/ng-bootstrap": "^1.0.0",
    "angular-gridster2": "^4.7.2",
    "bootstrap": "^4.3.1",
    "bourbon": "^5.0.0",
    "core-js": "^2.4.1",
    "font-awesome": "^4.7.0",
    "rxjs": "^5.5.2",
    "ssri": "^5.2.4",
    "zone.js": "^0.8.20"
  },
  "devDependencies": {
    "@angular/cli": "1.6.3",
    "@angular/compiler-cli": "^5.2.8",
    "@angular/language-service": "^5.2.8",
    "@commitlint/cli": "^5.2.8",
    "@commitlint/config-conventional": "^5.2.3",
    "@types/jasmine": "~2.5.53",
    "@types/jasminewd2": "~2.0.2",
    "@types/node": "^6.0.101",
    "codelyzer": "^4.2.1",
    "coveralls": "^3.0.0",
    "husky": "^0.14.3",
    "jasmine-core": "~2.6.2",
    "jasmine-spec-reporter": "~4.1.0",
    "karma": "~1.7.0",
    "karma-chrome-launcher": "~2.1.1",
    "karma-cli": "~1.0.1",
    "karma-coverage-istanbul-reporter": "^1.4.2",
    "karma-jasmine": "~1.1.0",
    "karma-jasmine-html-reporter": "^0.2.2",
    "prettier": "1.10.2",
    "pretty-quick": "^1.4.1",
    "protractor": "~5.1.2",
    "puppeteer": "^1.1.1",
    "ts-node": "~3.2.0",
    "tslint": "~5.7.0",
    "typescript": "~2.4.2"
  },
  "browserslist": {
    "demo1": ["defaults"],
    "prod": ["last 2 Chrome version"],
    "dev": ["last 2 Chrome version"]
  }
}
